{
  "name": "drupal/boilerplate",
  "description": "Drupal Boilerplate",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "php": ">=8.3",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/admin_toolbar": "^3.0",
    "drupal/captcha": "^2.0",
    "drupal/config_perms": "^2.0",
    "drupal/core-composer-scaffold": "^10.1",
    "drupal/core-project-message": "^10.1",
    "drupal/core-recommended": "^10.1",
    "drupal/easy_breadcrumb": "^2.0",
    "drupal/editor_advanced_link": "^2.0",
    "drupal/email_registration": "^1.1",
    "drupal/gin": "^3.0@RC",
    "drupal/gin_login": "^2.0",
    "drupal/gin_toolbar": "^1.0@RC",
    "drupal/mailsystem": "^4.3",
    "drupal/menu_trail_by_path": "^2.0",
    "drupal/metatag": "^1.22",
    "drupal/module_filter": "^4.0",
    "drupal/onlyone": "^2.0",
    "drupal/paragraphs": "^1.12",
    "drupal/pathauto": "^1.8",
    "drupal/redirect": "^1.6",
    "drupal/responsive_menu": "4.4.x-dev@dev",
    "drupal/shield": "^1.4",
    "drupal/symfony_mailer": "^1.3",
    "drupal/upgrade_status": "^4.0",
    "drupal/view_unpublished": "^1.0",
    "drupal/webform": "^6.2@beta",
    "drush/drush": "^12",
    "symfony/dotenv": "^5.3"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true
    }
  },
  "autoload": {
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "lup": "./scripts/local-update.sh",
    "rup": "./scripts/remote-update.sh"
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[web-root]/sites/development.services.yml": false
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {}
  }
}
