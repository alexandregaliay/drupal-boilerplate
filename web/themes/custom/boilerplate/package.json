{
  "name": "starter-bulma",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "description": "Uses Bulma for Drupal Boilerplate",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "cleanup": "rm -Rf node_modules dist && pnpm install"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.11.0+sha256.1c0e33f70e5df9eede84a357bdfa0b1f9dba6e58194628d48a1055756f553754",
  "dependencies": {
    "bulma": "^1.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.47",
    "sass": "^1.78.0",
    "vite": "^5.4.7"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": true,
      "cssnano": true
    }
  },
  "browserslist": [
    "defaults"
  ]
}
